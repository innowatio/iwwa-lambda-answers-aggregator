{
  "name": "lambda-boilerplate",
  "version": "1.0.0",
  "description": "Lambda boiler plate",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lk-architecture/lambda-boilerplate"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "dotenv": "^2.0.0",
    "kinesis-router": "^1.3.1",
    "mongodb": "^2.2.5"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.2.2",
    "isparta": "^4.0.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "build": "babel src -d bundle",
    "codecov": "codecov",
    "coverage": "NODE_ENV=test NODE_PATH=src babel-node $(npm bin)/isparta cover node_modules/mocha/bin/_mocha -- --recursive",
    "dev": "npm test -- --watch",
    "lint": "eslint src test",
    "test": "NODE_PATH=src NODE_ENV=test mocha --compilers js:babel-core/register --recursive"
  },
